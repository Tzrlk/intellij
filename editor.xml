<application>
  <component name="CodeVisionSettings">
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="2" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <settingsMapElement>
      <kotlin.kotlin.lambdas.hints>
        <Settings>
          <option name="implicitReceiversAndParams" value="false" />
        </Settings>
      </kotlin.kotlin.lambdas.hints>
    </settingsMapElement>
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>